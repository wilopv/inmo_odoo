<odoo>
    <data>
        <record id="view_inmueble_catastro_wizard" model="ir.ui.view">
            <field name="name">inmo.inmueble.catastro.wizard.form</field>
            <field name="model">inmo.inmueble.catastro.wizard</field>
            <field name="arch" type="xml">
                <form string="Consultar Catastro">
                    <group>
                        <field name="referencia_catastral"/>
                    </group>
                    <footer>
                        <button string="Aplicar"
                                type="object"
                                name="action_confirm"
                                class="btn-primary"/>
                        <button string="Cancelar" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="action_inmueble_catastro_wizard" model="ir.actions.act_window">
            <field name="name">Rellenar desde Catastro</field>
            <field name="res_model">inmo.inmueble.catastro.wizard</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_inmueble_catastro_wizard"/>
            <field name="target">new</field>
        </record>
    </data>
</odoo>
